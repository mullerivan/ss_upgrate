/* CSS RESET */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td { margin:0; padding:0; }
table { border-collapse:collapse; border-spacing:0; }
fieldset,img { border:0; }
address,caption,cite,code,dfn,em,strong,th,var { font-style:normal; font-weight:normal;}
ol,ul { list-style:none; }
caption,th { text-align:left; }
h1,h2,h3,h4,h5,h6 { font-size:100%; font-weight:normal; }
q:before,q:after { content:''; }
abbr,acronym { border:0; }
/* ****** */

body { margin:0; padding:0; text-align:center; background:#E2EBED; font:10pt "Trebuchet MS", Arial, Helvetica, sans-serif; line-height:14pt; }

h1 { font-size:14pt; font-weight:bold; margin:0 0 10px 0;  }
h2 { font-size:11pt; font-weight:bold; margin:0 0 5px 0;  }
small { font-size:8pt; }
big { font-size:20pt; line-height:normal;  }
strong { font-weight:bold; }
hr { margin:15px 0 15px 0; border:none; border-bottom:1px solid #578B9A; clear:both; }
a { color:#0E30DF; text-decoration:none; }
a:visited { color:#0E30DF; }
a:hover { text-decoration:underline; }
a:active {  }

.vcenter { vertical-align:middle; }
.clear { clear:both; }

#container { margin:0 auto; text-align:left; width:970px; padding:0 10px 10px 10px; background:#fff;   }

	#logo { width:300px; margin:0; line-height:50px; }
	#tagline { float:right; font-size:14pt; text-align:right; width:400px; line-height:30px; height:50px; padding:30px 0 0 0; vertical-align: bottom; }
	#banner { width:970; height:100px; }
	
#sidenavigation { width:180px; padding:0px; margin:5px; float:right;  }
	#sidenavigation ul {width:160px;  padding:10px;  background:#e8e8e8; }
		#sidenavigation ul li { border-bottom:1px solid #aaa; line-height:25px; text-indent:5px; }
		#sidenavigation ul li.first { border-top:1px dotted #aaa; }
		#sidenavigation ul li a { color:#000; text-decoration:none; display:block; text-decoration:none; }
		#sidenavigation ul li a:visited { color:#000; }
		#sidenavigation ul li a:hover {  color:#000; background:#ccc; display:block; }
		#sidenavigation ul li a:active {  }
		#sidenavigation ul li a.current { color:#000; background:#ccc; display:block; }
		#sidenavigation ul li a.section { color:#000; background:#ccc; display:block; }

#hznavigation { background:#555; height:25px; color:#FFF; border-top:1px solid #999; }
	#hznavigation ul li { float:left; border-right: 1px dotted #fff; line-height:25px; font-size:10pt; }
	#hznavigation ul li a { color:#fff; text-decoration:none; display:block; text-decoration:none; padding:0 10px 0 10px; }
	#hznavigation ul li a:visited { color:#FFF; }
	#hznavigation ul li a:hover { display:block; background:#FFF; color:#000; }
	#hznavigation ul li a:active {  }
	#hznavigation ul li a.current { color:#000; background:#fff; display:block; }
	#hznavigation ul li a.section { color:#000; background:#fff; display:block; }

	
#content { width:770px; float:left; overflow:auto;  }

#footer { line-height:25px; background:#578B9A; color:#fff; padding:0 10px 0 10px; text-align:center; font-size:9pt; }

#header_title {
	width: 100%;
	float: left;
	text-align: left;
	padding: 0 10px 10px 50px;
	font:25pt "Trebuchet MS", Arial, Helvetica, sans-serif; 
	font-weight: bold;
	color: white;
}

#header_tagline {
	width: 100%;
	float: right;
	text-align: right;
	padding: 0 10px 10px 22px;
	font:15pt "Trebuchet MS", Arial, Helvetica, sans-serif; 
	font-weight: bold;
	color: white;
}

#breadcrumb { padding-left: 0; color: #808080; font-size: 12pt; height: 25px; line-height: 25px;}

#breadcrumb a, #breadcrumb a:link, #breadcrumb a:visited {
	color: #668D18;
	font-size: 12pt;
}

#sidenavigation { width:180px; padding:0px; margin:5px; float:right;  }
	#sidenavigation ul#level3 {width:160px;  background:#e8e8e8; padding: 0;}
		#sidenavigation ul#level3 li { border: 0; line-height:25px; text-indent:15px; }
		#sidenavigation ul#level3 li a { color:#000; text-decoration:none; display:block; text-decoration:none; }
		#sidenavigation ul#level3 li a:visited { color:#000; }
		#sidenavigation ul#level3 li a:hover {  color:#000; background:#ddd; display:block; }
		#sidenavigation ul#level3 li a:active {  }
		#sidenavigation ul#level3 li a.current { color:#000; background:#ddd; display:block; }

.FooterContent a {
	color: white;
}